{
	"compilerOptions": {
		"noImplicitAny": false,
		"noImplicitThis": false,
		"noImplicitReturns": true,
		"strict": true,
		"alwaysStrict": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"noFallthroughCasesInSwitch": true,
		"removeComments": true,
		"sourceMap": false
	}
}
